{
	// See http://go.microsoft.com/fwlink/?LinkId=759670
	// for the documentation about the jsconfig.json format
	"compilerOptions": {
		"target": "es5",
		"sourceMap": true,
		"outDir": "dist",
		"experimentalDecorators": true,
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"pretty": true
	},
	"exclude": [
		"node_modules",
		"bower_components",
		"jspm_packages",
		"tmp",
		"temp",
		"bin",
		"examples"
	]
}
